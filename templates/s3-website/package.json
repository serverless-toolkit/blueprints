{
  "private": true,
  "name": "@serverless-toolkit/s3-website",
  "version": "0.0.1",
  "description": "Static HTML website hosted in S3",
  "scripts": {
    "deploy": "cdk deploy --context stackname=s3-website-example",
    "destroy": "cdk destroy --context stackname=s3-website-example --force",
    "clear": "rimraf cdk.out"
  },
  "devDependencies": {
    "ts-node": "10.9.1",
    "rimraf": "3.0.2",
    "constructs": "10.1.120",
    "@types/node": "18.8.0",
    "aws-cdk": "2.44.0",
    "aws-cdk-lib": "2.44.0"
  }
}
